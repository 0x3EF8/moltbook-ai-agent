# Environment variables for API keys
# Copy this file to .env and fill in your actual keys

# Moltbook API key - Get from https://www.moltbook.com/settings/developer
MOLTBOOK_API_KEY=your_moltbook_key_here

# Google Gemini API key - Get from https://ai.google.dev/
GEMINI_API_KEY=your_gemini_key_here

# Optional: Backup Gemini keys for auto-rotation (comma-separated)
GEMINI_BACKUP_KEYS=backup_key1,backup_key2,backup_key3
